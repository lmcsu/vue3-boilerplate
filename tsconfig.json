{
    "extends": "@vue/tsconfig/tsconfig.web.json",
    "include": [
        "./env.d.ts",
        "./src/**/*.ts",
        "./src/**/*.vue"
    ],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        },
        "noUncheckedIndexedAccess": true,
        "typeRoots": [
            "./node_modules/@types"
        ]
    }
}
